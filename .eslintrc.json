{
    "extends": ["next/core-web-vitals", "prettier"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "prefer-const": "error",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off"
    },
    "ignorePatterns": ["node_modules/", ".next/", "build/"]
}
